<MudDialog>
    <DialogContent>
        <MudText GutterBottom="true">Shared secret: @SharedSecret</MudText>
        <MudText GutterBottom="true">Auth secret: @AuthSecret</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton  Color="Color.Primary" OnClick="Submit">Ok</MudButton>
    </DialogActions>
</MudDialog>


@code {

    [CascadingParameter]
    MudDialogInstance MudDialog { get; set; }

    [Parameter]
    public string AuthSecret { get; set; }
    
    [Parameter]
    public string SharedSecret { get; set; }

    void Submit() => MudDialog.Close(DialogResult.Ok(true));

}